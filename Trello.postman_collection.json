		{
			"name": "Create New Board",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"//Status Code\r",
							"pm.test(\"Verify that the Status code is 200 and the Board added successfully\", function () \r",
							"{\r",
							"    pm.expect(pm.response.code).to.equal(200);\r",
							"});\r",
							"//Closed\r",
							"pm.test(\"Verify that closed is false\", function () \r",
							"{\r",
							"    pm.expect(pm.response.json().closed).to.equal(false);\r",
							"});\r",
							"//permissionLevel\r",
							"pm.test(\"Verify that permissionLevel is private\", function () \r",
							"{\r",
							"    pm.expect(pm.response.json().prefs.permissionLevel).to.equal(\"private\");\r",
							"});\r",
							"\r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
