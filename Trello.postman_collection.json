		{
			"name": "Get All Open Boards",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"//Status Code\r",
							"pm.test(\"Verify that the Status code is 200 and all Open Boards returns \", function () \r",
							"{\r",
							"    pm.expect(pm.response.code).to.equal(200);\r",
							"});\r",
							"\r",
							"// Response array size for Boards number should more than or equal 12\r",
							"pm.test(\"Check if response array size for Boards number is 5\",  () => {\r",
							"pm.expect(Object.keys(pm.response.json().boards).length).to.eql(4);\r",
							"});\r",
							"// first board\r",
							"pm.test(\"Verify that the first board is {FWD Board}\"), function () \r",
							"{\r",
							"    pm.expect(pm.response.json().boards[0].name).to.equal(\"FWD Board\");\r",
							"}\r",
							"//last board\r",
							"pm.test(\"Verify that the last board is {FWD Board}\", function () \r",
							"{\r",
							"    pm.expect(pm.response.json().boards[2].name).to.equal(\"FWD Board\");\r",
							"});\r",
							"\r",
							"\r",
							"\r",
							""
						],
